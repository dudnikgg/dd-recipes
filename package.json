{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently \"pnpm run dev:db\" \"nuxt dev\"",
    "dev:db": "turso dev --db-file local.db",
    "db:generate": "pnpm drizzle-kit generate",
    "db:migrate": "pnpm drizzle-kit migrate",
    "db:studio": "pnpm drizzle-kit studio",
    "db:seed": "cross-env DB_SEEDING=true tsx lib/db/seed.ts",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "nuxi typecheck",
    "prepare": "husky"
  },
  "dependencies": {
    "@libsql/client": "^0.15.9",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "1.13.0",
    "@nuxt/image": "1.10.0",
    "@nuxtjs/color-mode": "3.5.2",
    "@pinia/nuxt": "0.11.1",
    "@tailwindcss/vite": "^4.1.10",
    "@vee-validate/nuxt": "^4.15.1",
    "@vee-validate/zod": "^4.15.1",
    "@vercel/analytics": "^1.5.0",
    "@vue/typescript-plugin": "^2.2.10",
    "better-auth": "^1.3.3",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.7.1",
    "eslint": "^9.29.0",
    "gsap": "^3.13.0",
    "nuxt": "^4.0.0",
    "nuxt-csurf": "1.6.5",
    "nuxt-typed-router": "4.0.1",
    "pinia": "^3.0.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.10",
    "vue": "^3.5.17",
    "vue-multiselect": "^3.2.0",
    "vue-router": "^4.5.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.15.0",
    "@types/slug": "^5.0.9",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "daisyui": "^5.0.43",
    "drizzle-kit": "^0.31.1",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nanoid": "^5.1.5",
    "slug": "^11.0.0",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    },
    "overrides": {
      "sharp": "0.33.4"
    }
  },
  "lint-staged": {
    "*": "pnpm lint"
  }
}
